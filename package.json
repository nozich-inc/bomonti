{
  "name": "bomonti",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env next dev",
    "socket": "env-cmd -f .env nodemon --watch server.mjs server.mjs",
    "build": "env-cmd -f .env next build",
    "start": "env-cmd -f .env next start",
    "lint": "env-cmd -f .env next lint",
    "prisma": "env-cmd -f .env prisma",
    "pre:dev": "pnpm prisma generate",
    "pre:build": "pnpm i && pnpm add sharp && pnpm prisma generate && pnpm build",
    "prisma:dev": "pnpm prisma migrate dev",
    "prisma:deploy": "pnpm prisma migrate deploy",
    "update:packages": "pnpm update -r && pnpm add next@latest react@latest react-dom@latest @prisma/client@latest && pnpm update:dev",
    "update:dev": "pnpm add -D prisma@latest eslint@latest eslint-config-next@latest autoprefixer@latest postcss@latest tailwindcss@latest"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.1",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.17",
    "@prisma/client": "5.2.0",
    "bcrypt": "^5.1.1",
    "clsx": "^1.2.1",
    "dateformat": "^5.0.3",
    "devextreme": "~23.1.4",
    "devextreme-react": "~23.1.4",
    "encoding": "^0.1.13",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "framer-motion": "^10.16.1",
    "googleapis": "^118.0.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.1",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "node-cron": "^3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.10.1",
    "server-only": "^0.0.1",
    "sweetalert2": "11.4.8",
    "tailwind-merge": "^1.14.0",
    "ws": "^8.13.0",
    "xml2js": "^0.5.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "autoprefixer": "10.4.15",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "nodemon": "^3.0.1",
    "postcss": "8.4.28",
    "prisma": "5.2.0",
    "tailwindcss": "3.3.3"
  }
}